package 변수3_개념;

public class 변수3_개념03_논리연산자 {

	public static void main(String[] args) {
		System.out.println(10 == 10 && 3 == 3);
		System.out.println(10 != 10 && 3 == 3);
		System.out.println(10 == 10 && 3 != 3);
		System.out.println(10 != 10 && 3 != 3);
		
		System.out.println("--------------------");
		
		int a = 10;
		int b = 3;
		
		System.out.println(a == a || b == b);
		System.out.println(a != a || b == b);
		System.out.println(a == a || b != b);
		System.out.println(a != a || b != b);
        
		System.out.println("--------------------");
		
		System.out.println(!true);
		System.out.println(!false);
	}

}
