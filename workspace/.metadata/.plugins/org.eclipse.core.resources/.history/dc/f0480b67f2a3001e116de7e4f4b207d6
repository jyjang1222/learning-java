package 변수3_개념;

public class 변수3_개념01_비교연산자 {

	public static void main(String[] args) {
        System.out.println(10 == 3);
        System.out.println(10 != 3);
        System.out.println(10 > 3);
        System.out.println(10 < 3);
        System.out.println(10 >= 3);
        System.out.println(10 <= 3);
		    
        
        System.out.println("-----------------------");
        
        int a = 10;
        int b = 3;
        
        System.out.println(a == b);
        System.out.println(a != b);
        System.out.println(a > b);
        System.out.println(a < b);
        System.out.println(a >= b);
        System.out.println(a <= b);		
	}

}
